{% extends "enupal-stripe/_layouts/elementindex" %}
{% set title = "Orders"|t %}
{% set elementType = 'enupal\\stripe\\elements\\Order' %}
{% set currencies = craft.enupalStripe.getOrderCurrencies() %}
{% do view.registerAssetBundle("enupal\\stripe\\web\\assets\\FontAwesomeAsset") %}
{% do view.registerAssetBundle("enupal\\stripe\\web\\assets\\OrdersAsset") %}

{% block content %}
    <input type="hidden" id ="currencies" name="currencies" value="{{ currencies }}">
    {{ parent() }}
{% endblock %}

